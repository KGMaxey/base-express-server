{
  "name": "base-express-server",
  "version": "1.0.0",
  "description": "A simple base repo for initializing a typescript based express server including route structures and singleton services",
  "main": "dist/app.js",
  "_moduleAliases": {
    "@services": "dist/src/services",
    "@server": "dist/src/server"
  },
  "dependencies": {
    "@types/express": "^4.17.2",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "module-alias": "^2.2.2"
  },
  "devDependencies": {
    "ts-node": "^8.5.3",
    "typescript": "^3.7.2"
  },
  "scripts": {
    "build": "tsc",
    "start": "node -r module-alias/register .",
    "debug": "node --inspect-brk -r ts-node/register -r module-alias/register app.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KGMaxey/base-express-server.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/KGMaxey/base-express-server/issues"
  },
  "homepage": "https://github.com/KGMaxey/base-express-server#readme"
}
