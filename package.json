{
  "name": "base-express-server",
  "version": "2.0.0",
  "description": "A simple base repo for initializing a typescript based express server including route structures and singleton services",
  "main": "dist/app.js",
  "dependencies": {
    "@types/express": "^4.17.6",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "tsconfig-paths": "^3.9.0"
  },
  "devDependencies": {
    "ts-node": "^8.9.0",
    "typescript": "^3.8.3"
  },
  "scripts": {
    "build": "tsc && cp ./tsconfig.json dist",
    "serve": "cd dist/ && node -r tsconfig-paths/register src/app.js",
    "debug": "node --inspect-brk -r ts-node/register -r tsconfig-paths/register src/app.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KGMaxey/base-express-server.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/KGMaxey/base-express-server/issues"
  },
  "homepage": "https://github.com/KGMaxey/base-express-server#readme"
}
